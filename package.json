  "description": "AI提效工具应用",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "scripts": {
    "dev": "next dev -p 30011",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:ts": "tsc main.ts --outDir dist",
    "build:all": "pnpm build:next && pnpm build:electron",
    "build:next": "next build",
    "build:electron": "pnpm convert:main && electron-builder --win --config electron-builder.yml",
    "build:optimized": "cross-env NODE_ENV=production next build && electron-builder --win --config electron-builder.yml",
    "build:fast": "pnpm convert:main && electron-builder --win --config electron-builder.yml --dir",
    "build:ultrafast": "pnpm convert:main && electron-builder --win --config fast-electron-builder.yml",
    "optimize": "node scripts/optimize-build.js",
    "optimize:deps": "node scripts/dependency-optimizer.js",
    "clean:cache": "node scripts/clean-cache.js",
    "clean:logs": "node scripts/remove-debug-logs.js",
    "build:quick": "pnpm optimize && pnpm build:fast",
    "build:super-fast": "pnpm optimize && pnpm optimize:deps && pnpm build:fast",
    "build:max-speed": "pnpm optimize && pnpm optimize:deps && pnpm build:ultrafast",
    "build:clean": "pnpm clean:cache && pnpm build:max-speed",
    "build:safe": "pnpm clean:cache && pnpm build:simple",
    "build:production": "pnpm clean:logs && pnpm build:optimized",
    "start:electron": "electron .",
    "start2": "tsc-watch --noClear --onSuccess 'electron .' ",
    "convert:main": "babel  main.js  --out-file ./dist/main.cjs  --presets=@babel/preset-env   --plugins=@babel/plugin-transform-modules-commonjs"
  },
